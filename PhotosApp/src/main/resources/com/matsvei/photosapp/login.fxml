<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml" fx:controller="com.matsvei.photosapp.login.LoginController">
   <children>
      <Button fx:id="signupButton" layoutX="263.0" layoutY="269.0" mnemonicParsing="false" onAction="#onSignup" text="Sign up" />
      <TextField fx:id="usernameField" layoutX="220.0" layoutY="153.0" promptText="Username" />
      <Button fx:id="loginButton" layoutX="267.0" layoutY="236.0" mnemonicParsing="false" onAction="#onLogin" text="Log in" />
      <TextField fx:id="passwordField" layoutX="221.0" layoutY="187.0" promptText="Password" />
   </children>
</AnchorPane>
